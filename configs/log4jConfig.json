{
    "appenders": {
        "ruleConsole": {
            "type": "console"
        },
        "ruleFile": {
            "type": "dateFile",
            "filename": "logs/access.server.log",
            "maxLogSize": 1024,
            "backups": 3,
            "category": "normal"
        }
    },
    "categories": {
        "default": {
            "appenders": [
                "ruleConsole",
                "ruleFile"
            ],
            "level": "info"
        }
    }
}